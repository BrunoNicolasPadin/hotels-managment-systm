{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "binarytorch/larecipe": "^2.6",
        "cviebrock/eloquent-sluggable": "^10.0",
        "cybercog/laravel-ban": "^4.8",
        "dompdf/dompdf": "^2.0",
        "enlightn/enlightn": "^2.3",
        "guzzlehttp/guzzle": "^7.2",
        "hammerstone/fast-paginate": "^1.0",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.8",
        "laraveldaily/laravel-charts": "^0.2.1",
        "lorisleiva/laravel-actions": "^2.6",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.8",
        "mckenziearts/laravel-notify": "^2.4",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-health": "^1.22",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-query-builder": "^5.2",
        "spatie/laravel-translatable": "^6.5",
        "tailflow/laravel-orion": "^2.21"
    },
    "require-dev": {
        "beyondcode/laravel-query-detector": "^1.7",
        "fakerphp/faker": "^1.9.1",
        "laravel-shift/blueprint": "^2.7",
        "laravel/breeze": "^1.21",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.6",
        "nunomaduro/phpinsights": "^2.8",
        "pestphp/pest": "^2.8",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
